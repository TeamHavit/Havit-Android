<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>
        <variable
            name="vm"
            type="org.sopt.havit.util.web.DigBroadcastReceiver" />

    </data>

    <LinearLayout
        android:id="@+id/ll_webview"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/white_gray"
        android:gravity="center">

        <ImageView
            android:id="@+id/iv_webview_unread"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />


        <TextView
            android:id="@+id/tv_webview_unread"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginVertical="12dp"
            android:textAppearance="@style/Sub13Regular"
            android:textColor="@color/gray_2"
            android:text="@string/webview_unread" />
    </LinearLayout>
</layout>